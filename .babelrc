{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        "annotate-pure-calls",
        [
            "import",
            {
                "libraryName": "antd",
                "style": "css"
            }
        ],
        "@babel/plugin-proposal-class-properties",
        [
            "@babel/plugin-transform-runtime",
            {
                "regenerator": true,
            }
        ]
    ]
}